{
  "name": "wadi-api",
  "version": "5.1.0",
  "type": "commonjs",
  "scripts": {
    "dev": "nodemon -e ts,json --exec \"ts-node src/index.ts\"",
    "prebuild": "pnpm -C ../.. --filter @wadi/db-types build && pnpm -C ../.. --filter @wadi/core build && pnpm -C ../.. --filter @wadi/persona build",
    "build": "pnpm exec tsc --skipLibCheck",
    "start": "node dist/index.js",
    "test": "vitest",
    "regression-test": "ts-node src/scripts/regression-test.ts"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.86.0",
    "@types/express-rate-limit": "^6.0.2",
    "@wadi/core": "workspace:*",
    "@wadi/db-types": "workspace:*",
    "@wadi/persona": "workspace:*",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "express-rate-limit": "^8.2.1",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.2",
    "openai": "^6.9.1",
    "pdf-parse": "^2.4.5",
    "pino": "^10.1.0",
    "pino-pretty": "^13.1.3",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/helmet": "^4.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.10.1",
    "@types/pdf-parse": "^1.1.5",
    "@types/supertest": "^6.0.3",
    "nodemon": "^3.1.11",
    "supertest": "^7.2.2",
    "ts-node": "^10.9.2",
    "typescript": "~5.9.3",
    "vitest": "^4.0.16"
  }
}
